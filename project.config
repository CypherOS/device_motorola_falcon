[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Mati Halperin (MatiHalperin)
[access "refs/for/refs/*"]
	read = group user/Mati Halperin (MatiHalperin)
	push = group user/Mati Halperin (MatiHalperin)
	label-Code-Review = -2..+2 group user/Mati Halperin (MatiHalperin)
	label-Verified = -1..+1 group user/Mati Halperin (MatiHalperin)
	rebase = group user/Mati Halperin (MatiHalperin)
	removeReviewer = group user/Mati Halperin (MatiHalperin)
	submit = group user/Mati Halperin (MatiHalperin)
[access "refs/heads/*"]
	read = group user/Mati Halperin (MatiHalperin)
	create = group user/Mati Halperin (MatiHalperin)
	push = +force group user/Mati Halperin (MatiHalperin)
	pushMerge = group user/Mati Halperin (MatiHalperin)
[access "refs/tags/*"]
	push = group user/Mati Halperin (MatiHalperin)
	push = +force group user/Mati Halperin (MatiHalperin)
